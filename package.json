{
  "name": "strong-pwgen-cli",
  "version": "1.0.0",
  "description": "Generate cryptographically strong passwords.",
  "author": "Lars Graubner <mail@larsgraubner.de> (https://larsgraubner.com)",
  "homepage": "https://github.com/lgraubner/strong-pwgen-cli",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lgraubner/strong-pwgen-cli.git"
  },
  "bugs": {
    "url": "https://github.com/lgraubner/strong-pwgen-cli/issues"
  },
  "dependencies": {
    "clipboardy": "1.2.3",
    "mri": "1.1.0",
    "nanoid": "1.0.2"
  },
  "devDependencies": {
    "eslint": "4.18.2",
    "husky": "0.14.3",
    "lint-staged": "7.0.0",
    "prettier": "1.11.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "pwgen": "index.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "rules": {
      "no-console": 0
    },
    "env": {
      "node": true
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
